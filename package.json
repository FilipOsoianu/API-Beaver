{
  "name": "asigo-admin",
  "version": "4.0.1",
  "license": "MIT",
  "description": "asigo admin tool",
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --prod --aot",
    "lint": "ng lint",
    "lint:fix": "ng lint ngx-admin-demo --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@angular/animations": "^11.2.6",
    "@angular/cdk": "^11.2.5",
    "@angular/common": "^11.2.6",
    "@angular/compiler": "^11.2.6",
    "@angular/core": "^11.2.6",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.6",
    "@angular/material": "^11.2.5",
    "@angular/material-moment-adapter": "^11.2.5",
    "@angular/platform-browser": "^11.2.6",
    "@angular/platform-browser-dynamic": "^11.2.6",
    "@angular/router": "^11.2.6",
    "@nebular/auth": "7.0.0",
    "@nebular/eva-icons": "7.0.0",
    "@nebular/security": "7.0.0",
    "@nebular/theme": "7.0.0",
    "@ng-select/ng-select": "^6.1.0",
    "@rxweb/reactive-form-validators": "^2.1.2",
    "@swimlane/ngx-datatable": "^19.0.0",
    "angular2-image-upload": "^1.0.0-rc.2",
    "bootstrap": "4.6.0",
    "classlist.js": "1.1.20150312",
    "eva-icons": "^1.1.3",
    "file-saver": "^2.0.5",
    "moment": "^2.29.1",
    "ng-pick-datetime": "^7.0.0",
    "ng2-completer": "9.0.1",
    "ng2-smart-table": "1.7.2",
    "protractor": "^7.0.0",
    "rxjs": "6.6.6",
    "tslib": "^2.1.0",
    "yaml": "^2.0.0-4",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.5",
    "@angular/cli": "^11.2.5",
    "@angular/compiler-cli": "^11.2.6",
    "@types/node": "14.14.35",
    "codelyzer": "^6.0.1",
    "ts-node": "~9.1.1",
    "tslint": "^6.1.3",
    "typescript": "4.0.5"
  }
}
